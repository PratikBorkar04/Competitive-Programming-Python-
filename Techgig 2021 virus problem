   "source": [
    "#checks Whether your family is affected with any virus or not\n",
    "st1 = input(\"Enter Virus name:- \")\n",
    "st2 = input(\"Enter found Symptoms:-\")\n",
    "\n",
    "ls1 = []\n",
    "ls2 = []\n",
    "\n",
    "count = 0\n",
    "start = 0\n",
    "\n",
    "for i in st1:\n",
    "    ls1.append(i) # appending first input into first list\n",
    "    \n",
    "for j in st2:\n",
    "    ls2.append(j) # appending second input into second list\n",
    "    \n",
    "try :\n",
    "    for k in range(0,len(ls2)): # k loop for first list\n",
    "        for l in range(start,len(ls1)): # l loop for second list\n",
    "            if (ls1[l] == ls2 [k]): # checks whether any string is equal or not\n",
    "                start = l + 1 \n",
    "                count = count + 1\n",
    "                k = k + 1\n",
    "                break\n",
    "except Exception as ex:\n",
    "    print(ex)\n",
    "    \n",
    "if len(st2) == count:\n",
    "    print(\"+ve\")\n",
    "\n",
    "else:\n",
    "    print(\"-ve\")\n",
    "    \n",
    "# Sample input 1:- Coronavirus \n",
    "# Sample input 2:- vrus\n",
    "\n",
    "# Output : - +ve"
   ]
